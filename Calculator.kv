<CustButton@Button>: 

    font_size: 32
<calculatorGUI> 
    id: calculator 
    input: entry
    display: output
    # value:Buttons
    rows: 8
    padding: 10


    BoxLayout:
        spacing: 5
        
        TextInput:
            id: entry 
            background_color:(0,0,0,0)
            foreground_color:(1,1,1,1)
            font_size: 72
            multiline: False
    

    BoxLayout: 
        spacing: 5

        Label: 
            id: output 
            # size_hint: (1,None) 
            # height: 50
            font_size: 96
            multiline: False

    BoxLayout: 
        spacing: 1
        # size_hint:(.1,None)
        CustButton: 
            font_size: 72
            text: "7"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "8"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "9"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "+"
            on_press: root.inputDisplay(self.text) 

    BoxLayout: 
        spacing: 1
        # size_hint:(.1,None)

        
        CustButton: 
            font_size: 72
            text: "4"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "5"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "6"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "-"
            on_press: root.inputDisplay(self.text) 

    BoxLayout: 
        spacing: 1
        # size_hint:(.1,None)
        

        CustButton: 
            font_size: 72
            text: "1"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "2"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "3"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "*"
            on_press: root.inputDisplay(self.text) 

    BoxLayout: 
        spacing: 1
        # size_hint:(.1,None)

        
        CustButton: 
            font_size: 72
            text: "0"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "."
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "^"
            on_press: root.inputDisplay(self.text) 

        CustButton: 
            font_size: 72
            text: "/"
            on_press: root.inputDisplay(self.text) 

    BoxLayout: 
        spacing: 1
        # size_hint:(.1,None)
        
        CustButton: 
            font_size: 72
            text: "AC"
            on_press:  root.inputDisplay(self.text)

        CustButton: 
            font_size: 72
            text: "C"
            on_press: root.inputDisplay(self.text) 
        
        CustButton: 
            font_size: 72
            text: "="
            on_press: root.inputDisplay(self.text) 
        
        CustButton: 
            font_size: 72
            text: "%"
            # on_press: root.inputDisplay(self.text) 
